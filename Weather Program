#include <iostream>
#include <cmath>
using namespace std;

double windchillcalc(float T, float Y){
    return 35.74 + ((0.6215 * T) - (35.75*(pow(Y, 0.16)))) + (0.4275 * T * (pow(Y, 0.16)));
}

int main()
{
    
    float T, Y;
    
    cout << "Please enter degrees in fahrenheit..." << endl;
    cin >> T;
    
    cout << "Now, please enter wind speed in mph..." << endl;
    cin >> Y;
    
    double windchill = windchillcalc(T, Y);
   
    cout << "The wind chill is " << windchill << " degrees fahrenheit." << endl;
    return 0;
}


    
